{
  "name": "dpais-chat-template-pwa",
  "description": "Dell AI PC Chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "mkdir -p public && cp node_modules/pdfjs-dist/build/pdf.worker.mjs public/",
    "dev": "vite",
    "build": "tsc && vite build && npm run copy-pdf-worker",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "copy-pdf-worker": "cp node_modules/pdfjs-dist/build/pdf.worker.mjs dist/",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "generate-fixtures": "node tests/fixtures/generate-test-pdf.js",
    "test:vector-db": "mkdir -p public && cp node_modules/pdfjs-dist/build/pdf.worker.mjs public/ && vite",
    "test:db-status": "playwright test tests/e2e/database-status.spec.ts --project=chromium",
    "test:db-data": "playwright test tests/e2e/vector-db-data.spec.ts --project=chromium",
    "test:data-loader": "playwright test tests/e2e/data-loader.spec.ts --project=chromium",
    "test:docker-integration": "playwright test --config=playwright.docker.config.ts",
    "test:docker": "bash scripts/test-docker-integration.sh",
    "test:all": "yarn test:db-status && yarn test:db-data && yarn test:data-loader",
    "test:all-docker": "yarn test:db-status && yarn test:docker-integration",
    "test:all-dbs": "playwright test all-vector-dbs.spec.ts --project=chromium",
    "test:single-db": "playwright test vector-database.spec.ts --project=chromium",
    "backend:clean": "bash scripts/clean-containers.sh",
    "backend:setup-venv": "cd backend && python -m venv venv && . venv/bin/activate && pip install -r requirements.txt",
    "backend:start": "yarn backend:clean && cd backend && podman-compose up -d --build",
    "backend:stop": "cd backend && podman-compose down",
    "backend:logs": "cd backend && podman-compose logs -f",
    "backend:restart": "yarn backend:stop && yarn backend:start",
    "backend:mock": "cd backend && python3 mock_server.py",
    "backend:test": "cd backend && python -m pytest",
    "backend:test:unit": "cd backend && python -m pytest app/tests/test_health.py app/tests/test_vector_stores.py",
    "backend:test:integration": "cd backend && python -m pytest app/tests/test_integration.py -v",
    "backend:convert-compose": "scripts/docker-to-podman.sh podman backend/docker-compose.yml",
    "backend:convert-compose:reverse": "scripts/docker-to-podman.sh docker backend/podman-compose.yml",
    "backend:clear-data": "cd backend && . venv/bin/activate && python scripts/clear_vector_db.py --clear-collections",
    "backend:setup-backend:venv": "cd backend && . venv/bin/activate && pip install -r requirements.txt",
    "backend:load-missions": "cd backend && . venv/bin/activate && python scripts/load_mission_collections.py --collections-file ./scripts/mission_collections.json",
    "backend:load-data": "yarn backend:clear-data && yarn backend:load-missions",
    "start": "concurrently \"yarn backend:start\" \"yarn dev\"",
    "stop": "yarn backend:stop",
    "restart": "yarn backend:restart && yarn dev"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@langchain/community": "^0.3.36",
    "@langchain/core": "^0.3.42",
    "@langchain/openai": "0.5.5",
    "@langchain/pinecone": "^0.2.0",
    "@langchain/weaviate": "^0.1.1",
    "@mui/icons-material": "5.15.6",
    "@mui/material": "5.15.6",
    "@pinecone-database/pinecone": "^5.1.1",
    "@reduxjs/toolkit": "^2.6.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@types/xmldom": "^0.1.34",
    "chromadb": "^1.7.3",
    "concurrently": "^9.1.2",
    "jszip": "^3.10.1",
    "langchain": "^0.3.19",
    "motion": "^12.5.0",
    "openai": "^4.87.2",
    "pdfjs-dist": "^5.0.375",
    "pg": "^8.14.1",
    "qdrant": "^1.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "rxdb": "^16.8.0",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0",
    "weaviate-ts-client": "^2.2.0",
    "xlsx": "^0.18.5",
    "xmldom": "^0.6.0",
    "z-schema": "^6.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.51.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@vite-pwa/assets-generator": "^0.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "pdf-lib": "^1.17.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.11",
    "vite-plugin-pwa": "^0.21.1",
    "workbox-core": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "overrides": {
    "sharp": "0.32.6",
    "sharp-ico": "0.1.5"
  }
}
